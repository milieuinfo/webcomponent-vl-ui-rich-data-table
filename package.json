{
	"name": "vl-ui-rich-data-table",
	"version": "0.0.0",
	"description": "Een configureerbare data-table met paginering, sortering en zoeken op basis van een dynamische lijst van data.",
	"author": "DIDM",
	"license": "MIT",
	"main": "./src/index.js",
	"exports": {
		"require": "./index.js",
		"import": "./src/index.js",
		"node": "./index.js",
		"default": "./index.js"
	},
	"homepage": "https://github.com/milieuinfo/webcomponent-vl-ui-rich-data-table#readme",
	"bugs": {
		"url": "https://github.com/milieuinfo/webcomponent-vl-ui-rich-data-table/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/milieuinfo/webcomponent-vl-ui-rich-data-table.git"
	},
	"publishConfig": {
		"registry": "http://artifactory-pr-build.lb.cumuli.be:8081/artifactory/api/npm/acd-npm/"
	},
	"scripts": {
		"prepare": "npm update --dev vl-ui-util && npm run prepare:util",
		"prepare:util": "npm explore vl-ui-util -- npm run util",
		"test": "npm run test:unit && npm run test:e2e",
		"test:unit": "wct --config-file ./wct.local.conf.json",
		"test:e2e": "npm run test:server:start && npm run test:e2e:chrome && npm run test:e2e:firefox",
		"test:e2e:grid": "npm run test:server:start && npm run test:e2e:chrome:grid && npm run test:e2e:firefox:grid",
		"test:grid": "wct --config-file ./wct.docker.conf.json && npm run test:e2e:grid",
		"posttest:e2e": "pkill -f http-server",
		"test:e2e:chrome": "mocha --chrome",
		"test:e2e:chrome:grid": "mocha --chrome --grid",
		"test:e2e:firefox": "mocha --firefox",
		"test:e2e:firefox:grid": "mocha --firefox --grid",
		"test:e2e:opera": "mocha --opera",
		"test:e2e:safari": "mocha --safari",
		"test:server:start": "http-server &",
		"release": "np",
		"release:prepare": "npm explore vl-ui-util -- npm run release-component:prepare $(pwd) rich-data-table",
		"release:testless": "np --yolo",
		"demo": "http-server"
	},
	"dependencies": {
		"vl-ui-core": "^5.0.3",
		"vl-ui-icon": "^4.0.1",
		"vl-ui-data-table": "^3.1.0",
		"vl-ui-search-filter": "^3.1.0",
		"vl-ui-pager": "^3.0.1",
		"vl-ui-grid": "^3.0.1",
		"vl-ui-form-message": "^5.0.0"
	},
	"peerDependencies": {
		"vl-ui-core": "^5.0.3",
		"vl-ui-icon": "^4.0.1",
		"vl-ui-data-table": "^3.1.0",
		"vl-ui-search-filter": "^3.1.0",
		"vl-ui-pager": "^3.0.1",
		"vl-ui-grid": "^3.0.1",
		"vl-ui-form-message": "^5.0.0"
	},
	"devDependencies": {
		"@govflanders/vl-ui-util": "3.9.1",
		"vl-ui-util": "^5.0.9",
		"vl-ui-input-field": "^3.0.0"
	}
}
