{
  "name": "vl-rich-table",
  "version": "0.0.0",
  "description": "Een configureerbare data-table met paginering, sortering en zoeken op basis van een dynamische lijst van data.",
  "scripts": {
    "prepare": "npm update --dev vl-ui-util && npm run prepare:util",
    "prepare:util": "npm explore vl-ui-util -- npm run install:copy && npm explore vl-ui-util -- npm run install:bamboo",
    "test": "wct -l chrome,firefox --npm",
    "release": "np",
    "release:prepare": "npm run release:prepare:build && npm run release:prepare:css && npm run release:prepare:commit",
    "release:prepare:build": "npm run build",
    "release:prepare:css": "replace '\\.\\./style.css' '/node_modules/vl-ui-rich-table/style.css' vl-rich-table.js",
    "release:prepare:commit": "git add -f vl-rich-table.js && git commit --amend --no-edit && git pull",
    "release:testless": "np --yolo",
    "dev": "concurrently \"npm:bundle:watch\" \"http-server\"",
    "build": "npm run bundle:build && npm run bundle:build:imports",
    "bundle:watch": "rollup --config rollup.config.js --watch",
    "bundle:build": "rollup --config rollup.config.js",
    "bundle:build:imports": "replace '\"[../]+/node_modules' '\"/node_modules' vl-rich-table.js",
    "demo": "npm run dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/milieuinfo/webcomponent-vl-rich-table.git"
  },
  "author": "DIDM",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/milieuinfo/webcomponent-vl-rich-table/issues"
  },
  "homepage": "https://github.com/milieuinfo/webcomponent-vl-rich-table#readme",
  "dependencies": {
    "vl-ui-button": "1.3.2",
    "vl-ui-core": "^2.0.0",
    "vl-ui-data-table": "1.0.7",
    "vl-ui-grid": "2.0.2",
    "vl-ui-icon": "2.1.0",
    "vl-ui-pager": "0.0.4"
  },
  "devDependencies": {
    "@govflanders/vl-ui-util": "^3.9.1",
    "concurrently": "5.0.0",
    "http-server": "^0.11.1",
    "np": "3.0.4",
    "replace": "^1.1.1",
    "rollup": "1.25.1",
    "rollup-plugin-postcss": "2.0.3",
    "rollup-plugin-terser": "5.1.2",
    "sass": "1.23.6",
    "uglify-js-es6": "2.8.9",
    "vl-ui-datepicker": "github:milieuinfo/webcomponent-vl-ui-datepicker#change_event_propageren",
    "vl-ui-form-message": "3.0.0",
    "vl-ui-input-field": "1.1.2",
    "vl-ui-search-filter": "0.0.1",
    "vl-ui-select": "2.0.1",
    "vl-ui-util": "^3.4.4",
    "wct-browser-legacy": "1.0.2",
    "web-component-tester": "6.9.2"
  },
  "publishConfig": {
    "registry": "http://artifactory-pr-build.lb.cumuli.be:8081/artifactory/api/npm/acd-npm/"
  }
}
